[user]
	email = j.trew10@gmail.com
	name = James Trew
[core]
	editor = nvim
  pager = delta
[interactive]
  diffFilter = delta --color-only
[delta]
  navigate = true    # use n and N to move between diff sections
  light = false      # set to true if you're in a terminal w/ a light background color (e.g. the default macOS terminal)
  line-numbers = true
[merge]
  conflictstyle = diff3
[diff]
  colorMoved = default
[color]
	ui = true
[pull]
	rebase = true
[http]
	cookiefile = /home/jt/.gitcookies
[alias]
	bb = !better-git-branch.sh
[rerere]
	enabled = true
